<div class="row">
  <div class="col-md-6 col-md-offset-3">

    <h1 class="track-title"><%= @album.title %></h1>
    <p class="artist">Artist: <%= @album.artist %></p>
    
    <% @tracks.each do |track| %>
      <div class="track">
        <p><%= track.title %> Length: <%= track.duration %> mins</p>
      </div>
    <% end %>
    
  </div>
</div>